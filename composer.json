{
    "name": "lyranetwork/payment-ws-api",
    "description": "SOAP WS API to call Lyra payment services in PHP based websites.",
    "version": "1.0.0",
    "type": "library",
    "keywords": ["api", "ws", "soap", "wsdl", "sdk", "php", "payment", "e-commerce", "lyra"],
    "license": "GPL-3.0+",
    "authors": [{
        "name": "Lyra Network",
        "homepage": "https://www.lyra-network.com/"
    }],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.0",
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4" : {
            "Lyranetwork\\" : "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lyranetwork\\" : "lib/",
            "Tests\\": "tests/"
        }
    }
}